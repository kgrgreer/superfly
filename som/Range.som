Range = Parser (
  | start end |

  start: v = ( start := v )
  end:   v = ( end   := v )

  inRange: h = (
    ^ ( ( h >= start ) && [ ^ ( h <= end ) ] )
  )

  parse: ps = ( | h |
    h := (ps head) hashcode.
    (self inRange: h) ifTrue: [ ^ ps tail: (ps head) ].
    ^ nil
  )

  ----
  from: s to: e = ( | r |
    r := Range new.
    r start: (s charAt: 1) hashcode.
    r end:   (e charAt: 1) hashcode.
    ^r
  )
)
