Sym = Parser (
  | grammar symbol |

  grammar: v = ( grammar := v )
  symbol:  v = ( symbol  := v )

  parse: ps = (
    ('   ------------------------------ SYM: ' + symbol) println.
    ^ (grammar perform: symbol) parse: ps
  )

  ----
  grammar: g symbol: s = ( | p |
    p := Sym new.
    p grammar: g.
    p symbol: s.
    ^p
  )
)
