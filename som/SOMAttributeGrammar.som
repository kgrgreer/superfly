SOMAttributeGrammar = SOMParser (

  arrayToStr: a = (
    a class = Vector ifTrue: [ a := a asArray ].
    ('----atoS ' + a) println.
    ^ a inject: '' into: [ :str :elem | str + (elem asString) ]
  )

  Identifier = (
    ^ super Identifier action: [ :a |
    "a at: 1 + " self arrayToStr: (a at: 2) ]
  )

----

)
